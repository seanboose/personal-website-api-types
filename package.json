{
  "name": "@seanboose/personal-website-api-types",
  "version": "1.0.12",
  "description": "oapi schema and types for personal website frontend and backend",
  "license": "MIT",
  "author": "",
  "type": "commonjs",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanboose/personal-website-api-types.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "npm run format && npm run lint && npm run build:ts",
    "build:ts": "tsc && cp src/index.mjs dist/index.mjs && cp src/index.cjs dist/index.cjs",
    "format": "prettier --write .",
    "lint": "eslint src --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.4"
  },
  "dependencies": {
    "zod": "^4.1.12"
  }
}
